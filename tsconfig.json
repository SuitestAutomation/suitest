{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2017",
      "es2019"
    ],
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "compileOnSave": false
}
